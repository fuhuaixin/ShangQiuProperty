<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <include layout="@layout/include_activity_title" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="180dp"
        android:src="@mipmap/icon_change_nub_suc" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="手机号已更改为"
        android:textColor="@color/tv333"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="1254568794"
        android:textColor="@color/tv333"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tv_back"
        android:layout_width="170dp"
        android:layout_height="45dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/shape_f2_0_28"
        android:gravity="center"
        android:text="返回"
        android:textColor="@color/col_repairs_blue"
        android:textSize="17sp"
        android:textStyle="bold" />
</LinearLayout>